{
	"name" : "hanoivip/download",
	"description" : "Player download apps",
	"type" : "library",
	"license" : "MIT",
	"autoload" : {
		"psr-4" : {
			"Hanoivip\\Download\\" : "src/"
		}
	},
	"repositories" : [{
			"type" : "path",
			"url" : "../events",
			"options" : {
				"symlink" : true
			}
		}
	],
	"require" : {
		"php" : ">=7.0.0",
		"guzzlehttp/guzzle" : "^6.3",
		"laravel/framework" : ">=5.5",
		"laravel/tinker" : "~1.0",
		"jenssegers/agent" : "^2.6",
		"hanoivip/events" : "dev-master"
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Hanoivip\\Download\\LibServiceProvider"
			]
		}
	}
}